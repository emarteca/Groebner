#
#  Trinks Problem :  from Trinks, 1978
#
# This is baby food these days.
 
f1 := 45*p +  35*s - 165*b - 36:
f2 := 35*p + 40*z + 25*t - 27*s:
f3 := 15*w + 25*p*s + 30*z - 18*t - 165*b^2:
f4 := -9*w + 15*p*t + 20*z*s:
f5 := w*p + 2*z*t - 11*b^3:
f6 := 99*w - 11*s*b + 3*b^2:
F := [f1,f2,f3,f4,f5,f6]:

Xa := [w,p,z,t,s,b];  # easy for a lex. order
Xb := [w,b,p,z,s,t];  # a little harder
Xc := [b,t,s,w,p,z];  # harder still ... but perhaps not hard enough?

